<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="650.0" styleClass="background" stylesheets="@Theme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.InventoryReportScreenController">
   <children>
      <Label alignment="CENTER" layoutX="150.0" layoutY="50.0" prefHeight="60.0" prefWidth="350.0" text="Inventory Report" wrapText="true">
         <font>
            <Font name="Britannic Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="inventoryTable" layoutX="25.0" layoutY="150.0" prefHeight="450.0" prefWidth="620.0">
        <columns>
          <TableColumn fx:id="productID" prefWidth="110.0" text="ProductID" />
          <TableColumn fx:id="productName" prefWidth="150.0" text="Product Name" />
          <TableColumn fx:id="productDesc" prefWidth="160.0" text="Product Description" />
          <TableColumn fx:id="availableQuantity" prefWidth="160.0" text="Available Quantity" />
        </columns>
      </TableView>
      <Button fx:id="back" layoutX="550.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonAction" prefHeight="25.0" prefWidth="100.0" text="BACK" />
   </children>
</AnchorPane>
